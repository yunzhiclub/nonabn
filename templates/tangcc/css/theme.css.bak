/*分页样式*/
.pagination{text-align:center;clear:both;padding-right: 40px;}
.pagination ul{width:100%;margin-left:auto;margin-right:auto}
.pagination ul li{display:inline-block;margin:0px 4px;}
.pagination ul li span, .pagination ul li a{display:inline-block;}
.pagination ul li.pagination-start{padding: 0px;background: url('../images/j_button2_right.png') no-repeat scroll right top transparent;}
.pagination ul li.pagination-start span.pagenav{background: url('../images/j_button2_first_off.png') no-repeat scroll 0% 0% transparent;width: 40px;padding-left: 15px;}
.pagination ul li.pagination-start a.pagenav{background: url('../images/j_button2_first.png') no-repeat scroll 0% 0% transparent;width: 40px;padding-left: 15px;}
.pagination ul li.pagination-prev{background: url('../images/j_button2_right.png') no-repeat top right;}
.pagination ul li.pagination-prev a.pagenav{background: url('../images/j_button2_prev.png') no-repeat scroll 0% 0% transparent;width: 40px;padding-left: 15px;}
.pagination ul li.pagination-prev span.pagenav{background: url('../images/j_button2_prev_off.png') no-repeat scroll 0% 0% transparent;width: 40px;padding-left: 15px;}
.pagination ul li.pagination-end{padding: 0px;background: url('../images/j_button2_left.png') no-repeat scroll left top transparent;}
.pagination ul li.pagination-end span.pagenav{background: url('../images/j_button2_last_off.png') no-repeat scroll top right  transparent;width: 40px;padding-right: 15px;}
.pagination ul li.pagination-end a.pagenav{background: url('../images/j_button2_last.png') no-repeat scroll top right  transparent;width: 40px;padding-right: 15px;}
.pagination ul li.pagination-next{background: url('../images/j_button2_left.png') no-repeat top left;}
.pagination ul li.pagination-next a.pagenav{background: url('../images/j_button2_next.png') no-repeat scroll top right  transparent;width:40px;padding-right: 15px;}
.pagination ul li.pagination-next span.pagenav{background: url('../images/j_button2_next_off.png') no-repeat scroll top right  transparent;width: 40px;padding-right: 15px;}
.pagination ul li{background: url('../images/j_button2_left.png') no-repeat scroll 0% 0% transparent;}
.pagination ul li span.pagenav{width: 20px;background: url('../images/j_button2_right_cap.png') no-repeat scroll right top transparent;color: #ccc;}
.pagination ul li a.pagenav{width: 20px;background: url('../images/j_button2_right_cap.png') no-repeat scroll right top transparent;}
